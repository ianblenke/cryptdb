OBJDIRS	+= test

TEST_SRCS   :=  TestCrypto.cc \
		test_searchtree.cc \
		test_system.cc

		
all:	$(OBJDIR)/test/test_searchtree
$(OBJDIR)/test/test_searchtree: $(OBJDIR)/test/test_searchtree.o $(OBJDIR)/libcryptdb.so $(OBJDIR)/libedbutil.so $(OBJDIR)/libope.so
	$(CXX) $< -o $@ $(LDFLAGS) -ledbutil -lope -lcryptdb

all:	$(OBJDIR)/test/test
$(OBJDIR)/test/test: $(OBJDIR)/test/test_system.o $(OBJDIR)/libcryptdb.so $(OBJDIR)/libedbutil.so $(OBJDIR)/libope.so
	$(CXX) $< -o $@ $(LDFLAGS) -ledbutil -lope -lcryptdb

