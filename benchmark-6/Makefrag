OBJDIRS += benchmark-6
BENCHMARK_6PROGS := program
BENCHMARK_6PROGOBJS := $(patsubst %,$(OBJDIR)/benchmark-6/%,$(BENCHMARK_6PROGS))
all: $(BENCHMARK_6PROGOBJS)
$(BENCHMARK_6PROGOBJS): %: %.o $(OBJDIR)/libcryptdb.so $(OBJDIR)/libedbparser.so $(OBJDIR)/libedbutil.so $(OBJDIR)/libexecution.so
	$(CXX) $< -o $@ -ledbparser  $(LDFLAGS) -ledbutil -lcryptdb -ledbcrypto -ledbcrypto2 -lexecution -lmysqlclient -ltbb -lgmp -lpq
