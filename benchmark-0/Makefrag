OBJDIRS += benchmark-0
BENCHMARK_0PROGS := program
BENCHMARK_0PROGOBJS := $(patsubst %,$(OBJDIR)/benchmark-0/%,$(BENCHMARK_0PROGS))
all: $(BENCHMARK_0PROGOBJS)
