OBJDIRS += benchmark-13
BENCHMARK_13PROGS := program
BENCHMARK_13PROGOBJS := $(patsubst %,$(OBJDIR)/benchmark-13/%,$(BENCHMARK_13PROGS))
all: $(BENCHMARK_13PROGOBJS)
$(BENCHMARK_13PROGOBJS): %: %.o $(OBJDIR)/libcryptdb.so $(OBJDIR)/libedbparser.so $(OBJDIR)/libedbutil.so $(OBJDIR)/libexecution.so
	$(CXX) $< -o $@ -ledbparser  $(LDFLAGS) -ledbutil -lcryptdb -ledbcrypto -ledbcrypto2 -lexecution -lmysqlclient -ltbb -lgmp -lpq
