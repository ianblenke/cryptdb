OBJDIRS += benchmark-1
BENCHMARK_1PROGS := program
BENCHMARK_1PROGOBJS := $(patsubst %,$(OBJDIR)/benchmark-1/%,$(BENCHMARK_1PROGS))
all: $(BENCHMARK_1PROGOBJS)
$(BENCHMARK_1PROGOBJS): %: %.o $(OBJDIR)/libcryptdb.so $(OBJDIR)/libedbparser.so $(OBJDIR)/libedbutil.so $(OBJDIR)/libexecution.so
	$(CXX) $< -o $@ -ledbparser  $(LDFLAGS) -ledbutil -lcryptdb -ledbcrypto -ledbcrypto2 -lexecution -lmysqlclient -ltbb -lgmp -lpq
