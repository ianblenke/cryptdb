OBJDIRS += benchmark-16
BENCHMARK_16PROGS := program
BENCHMARK_16PROGOBJS := $(patsubst %,$(OBJDIR)/benchmark-16/%,$(BENCHMARK_16PROGS))
all: $(BENCHMARK_16PROGOBJS)
$(BENCHMARK_16PROGOBJS): %: %.o $(OBJDIR)/libcryptdb.so $(OBJDIR)/libedbparser.so $(OBJDIR)/libedbutil.so $(OBJDIR)/libexecution.so
	$(CXX) $< -o $@ -ledbparser  $(LDFLAGS) -ledbutil -lcryptdb -ledbcrypto -ledbcrypto2 -lexecution -lmysqlclient -ltbb -lgmp -lpq
