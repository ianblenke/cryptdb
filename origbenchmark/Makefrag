OBJDIRS += origbenchmark
ORIGBENCHMARKPROGS := program
ORIGBENCHMARKPROGOBJS := $(patsubst %,$(OBJDIR)/origbenchmark/%,$(ORIGBENCHMARKPROGS))
all: $(ORIGBENCHMARKPROGOBJS)
$(ORIGBENCHMARKPROGOBJS): %: %.o $(OBJDIR)/libcryptdb.so $(OBJDIR)/libedbparser.so $(OBJDIR)/libedbutil.so $(OBJDIR)/libexecution.so
	$(CXX) $< -o $@ -ledbparser  $(LDFLAGS) -ledbutil -lcryptdb -ledbcrypto -ledbcrypto2 -lexecution -lmysqlclient -ltbb -lgmp -lpq
