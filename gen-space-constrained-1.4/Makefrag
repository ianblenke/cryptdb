OBJDIRS += gen-space-constrained-1.4
SPACE_CONSTRAINED_14PROGS := program 
SPACE_CONSTRAINED_14PROGOBJS := $(patsubst %,$(OBJDIR)/gen-space-constrained-1.4/%,$(SPACE_CONSTRAINED_14PROGS))
all: $(SPACE_CONSTRAINED_14PROGOBJS)
$(SPACE_CONSTRAINED_14PROGOBJS): %: %.o $(OBJDIR)/libcryptdb.so $(OBJDIR)/libedbparser.so  $(OBJDIR)/libedbutil.so $(OBJDIR)/libexecution.so
	$(CXX) $< -o $@ -ledbparser  $(LDFLAGS) -ledbutil -lcryptdb -ledbcrypto -ledbcrypto2 -lexecution -lmysqlclient -ltbb -lgmp -lpq
